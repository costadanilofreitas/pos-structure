-- Schema: i18ncustom
-- Brief: customized internationalization and localization message tables
-- Created by: amerolli
-- Table schema_version: must be defined for database update through the Persistence Component
CREATE TABLE schema_version AS
SELECT "$Author$" AS Author,
       "$Date$" AS LastModifiedAt,
       "$Revision: 2131abc1aca7364d769654398689ee3fff6ea49d$" AS Revision;

CREATE TABLE Languages (
    LangCode        INTEGER PRIMARY KEY AUTOINCREMENT,
    LangName        VARCHAR NOT NULL UNIQUE,
    ParentLangCode  INTEGER DEFAULT NULL
    CONSTRAINT FK_Languages_ParentLangCode REFERENCES Languages(LangCode)
);
CREATE TABLE Messages (
    LangCode    INTEGER NOT NULL
    CONSTRAINT FK_Messages_LangCode REFERENCES Languages(LangCode),
    MessageKey  VARCHAR,
    Message     VARCHAR,
    PRIMARY KEY(LangCode, MessageKey)
);

