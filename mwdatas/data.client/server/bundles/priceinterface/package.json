  "description": "POS Price UI",
  "main": "index.js",
  "repository": "https://bitbucket.org/edeployteam/mwpos.git",
  "scripts": {
    "build": "webpack  --config webpack.prod.config.js --optimize-minimize --progress --colors --profile --define process.env.NODE_ENV='\"production\"'",
    "dev": "webpack --progress --colors --define process.env.NODE_ENV='\"development\"'",
    "zip": "powershell.exe -nologo -noprofile -command \"If (Test-Path priceinterface.zip) { Remove-Item priceinterface.zip } & { Add-Type -A 'System.IO.Compression.FileSystem'; [IO.Compression.ZipFile]::CreateFromDirectory('dist', 'priceinterface.zip'); }\"",
    "deploy": "npm run build && npm run zip & move /Y priceinterface.zip ..\\..\\bin"
  },
