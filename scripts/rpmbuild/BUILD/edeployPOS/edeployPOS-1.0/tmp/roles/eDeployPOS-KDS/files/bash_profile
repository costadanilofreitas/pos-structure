# .bash_profile

# Get the aliases and functions
if [ -f ~/.bashrc ]; then
        . ~/.bashrc
fi

# User specific environment and startup programs

PATH=$PATH:$HOME/.local/bin:$HOME/bin:$HOME/edeployPOS_kds/bin
LD_LIBRARY_PATH=$LD_LIBRARY_PATH:$HOME/edeployPOS_kds/bin:$HOME/edeployPOS_kds/python/lib

export PATH
export LD_LIBRARY_PATH
