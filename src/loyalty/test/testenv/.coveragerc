[run]
omit =
		../../test/*
		../../lib/*
		../../dev_lib/*
		../../src/externals/*
		../../src/main.py
source = 
		../../src
branch=True

[report]
exclude_lines =
		pragma: no cover
		def __repr__
		if self.debug:
		if settings.DEBUG
		raise AssertionError
		raise NotImplementedError
		if 0:
		if __name__ == .__main__.:
